{
  "ver": "0.0.1",
  "authors": ["kkjeer"],
  "contact": "https://kbase.us/contact-us/",
  "categories": ["active"],
  "widgets": {
    "input": null,
    "output": "no-display"
  },
  "parameters": [
    {
      "id": "table_id",
      "optional": true,
      "advanced": true,
      "allow_multiple": false,
      "default_values": [""],
      "field_type": "text",
      "text_options": {
        "valid_ws_types": ["MAK.StringDataTable"]
      }
    },
    {
      "id": "mapping_id",
      "optional": false,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [""],
      "field_type": "text",
      "text_options": {
        "valid_ws_types": ["KBaseExperiments.AttributeMapping"]
      }
    },
    {
      "text_options": {
        "valid_ws_types": ["KBaseFBA.FBAModel"]
      },
      "advanced": true,
      "default_values": [""],
      "id": "fbamodel_id",
      "optional": true,
      "allow_multiple": false,
      "field_type": "text"
    },
    {
      "id": "target_reaction",
      "field_type": "textsubdata",
      "allow_multiple": false,
      "optional": true,
      "advanced": true,
      "default_values": ["bio1"],
      "text_options": {
        "valid_ws_types": []
      },
      "textsubdata_options": {
        "show_src_obj": true,
        "allow_custom": true,
        "multiselection": false,
        "subdata_selection": {
          "additional_options": ["bio1 - Biomass"],
          "subdata_included": [
            "modelreactions/[*]/id",
            "modelreactions/[*]/name"
          ],
          "selection_description": ["name"],
          "selection_id": "id",
          "path_to_subdata": ["modelreactions"],
          "parameter_id": "fbamodel_id"
        }
      }
    },
    {
      "id": "test_run",
      "optional": true,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [""],
      "field_type": "text"
    },
    {
      "id": "row_id",
      "field_type": "textsubdata",
      "allow_multiple": false,
      "optional": false,
      "advanced": false,
      "default_values": [""],
      "text_options": {
        "valid_ws_types": []
      },
      "textsubdata_options": {
        "show_src_obj": true,
        "allow_custom": true,
        "multiselection": false,
        "subdata_selection": {
          "additional_options": [],
          "subdata_included": [
            "attributes/[*]/attribute",
            "attributes/[*]/unit"
          ],
          "selection_description": ["attribute", "unit"],
          "description_template": "run id: {{attribute}}, objective value: {{unit}}",
          "selection_id": "attribute",
          "path_to_subdata": ["attributes"],
          "parameter_id": "mapping_id"
        }
      }
    },
    {
      "id": "table_row_id",
      "field_type": "textsubdata",
      "allow_multiple": false,
      "optional": true,
      "advanced": true,
      "default_values": [""],
      "text_options": {
        "valid_ws_types": []
      },
      "textsubdata_options": {
        "show_src_obj": true,
        "allow_custom": true,
        "multiselection": false,
        "subdata_selection": {
          "additional_options": [],
          "subdata_included": ["row_ids/[*]"],
          "selection_description": [""],
          "selection_id": "",
          "path_to_subdata": ["row_ids"],
          "parameter_id": "table_id"
        }
      }
    },
    {
      "id": "feedback",
      "optional": false,
      "allow_multiple": false,
      "default_values": ["unknown"],
      "field_type": "dropdown",
      "dropdown_options": {
        "options": [
          {
            "value": "unknown",
            "display": "Unknown",
            "id": "unknown",
            "ui_name": "Unknown"
          },
          {
            "value": "correct",
            "display": "Correct",
            "id": "correct",
            "ui_name": "Correct"
          },
          {
            "value": "incorrect",
            "display": "Incorrect",
            "id": "incorrect",
            "ui_name": "Incorrect"
          }
        ]
      }
    }
  ],
  "parameter-groups": [
    {
      "id": "param_group",
      "parameters": ["test_run", "row_id", "feedback"],
      "optional": false,
      "allow_multiple": true,
      "with_border": true
    }
  ],
  "behavior": {
    "service-mapping": {
      "url": "",
      "name": "TestFeedback",
      "method": "run_TestFeedback",
      "input_mapping": [
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        },
        {
          "narrative_system_variable": "workspace_id",
          "target_property": "workspace_id"
        },
        {
          "input_parameter": "fbamodel_id",
          "target_property": "fbamodel_id"
        },
        {
          "input_parameter": "target_reaction",
          "target_property": "target_reaction"
        },
        {
          "input_parameter": "table_id",
          "target_property": "table_id"
        },
        {
          "input_parameter": "mapping_id",
          "target_property": "mapping_id"
        },
        {
          "input_parameter": "param_group",
          "target_property": "param_group"
        }
      ],
      "output_mapping": [
        {
          "service_method_output_path": [0, "report_name"],
          "target_property": "report_name"
        },
        {
          "service_method_output_path": [0, "report_ref"],
          "target_property": "report_ref"
        },
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        }
      ]
    }
  },
  "job_id_output_field": "docker"
}
